{
  "expo": {
    "name": "Absenify",
    "slug": "absenify",
    "scheme": "absenify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tes3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/tes2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.suta.absenify"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.suta.absenify"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4874ba6e-b247-45d1-a057-62aefe4a3ec0"
      }
    },
    "owner": "sutaaa"
  }
}
